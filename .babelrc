{
    "presets": ["es2015", "react"],
    "plugins": [
        ["transform-runtime"],
        ["transform-decorators-legacy"],
        ["transform-class-properties"]
    ],
    "env": {
        "development": {
            "plugins": [
                ["react-transform", {
                    "transforms": [
                        {
                            "transform": "react-transform-hmr",
                            "imports": ["react"],
                            "locals": ["module"]
                        }
                    ]
                }]
            ]
        }
    }
}
